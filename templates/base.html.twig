<!DOCTYPE html>
<html>
  <head>
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    {% block seo %}
    {{
      sonata_seo_title()
    }}
    {{
      sonata_seo_metadatas()
    }}
    {{
      sonata_seo_link_canonical()
    }}
    {% endblock %}
    <!-- Favicon -->
    <link
      href="{{ asset('frontoffice/') }}/images/favicon.png"
      rel="shortcut icon"
    />
    <!-- CSS -->
    <link
      href="{{ asset('frontoffice/') }}plugins/bootstrap/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      href="{{
        asset('frontoffice/')
      }}plugins/owl-carousel/owl.carousel.min.css"
      rel="stylesheet"
    />
    <link
      href="{{
        asset('frontoffice/')
      }}plugins/owl-carousel/owl.theme.default.min.css"
      rel="stylesheet"
    />
    <link
      href="{{
        asset('frontoffice/')
      }}plugins/magnific-popup/magnific-popup.min.css"
      rel="stylesheet"
    />
    <link
      href="{{ asset('frontoffice/') }}plugins/sal/sal.min.css"
      rel="stylesheet"
    />
    <link href="{{ asset('frontoffice/') }}css/theme.css" rel="stylesheet" />
    <!-- Fonts/Icons -->
    <link
      href="{{
        asset('frontoffice/')
      }}plugins/bootstrap-icons/bootstrap-icons.css"
      rel="stylesheet"
    />
    <link
      href="{{ asset('frontoffice/') }}plugins/font-awesome/css/all.css"
      rel="stylesheet"
    />
    {# Run `composer require symfony/webpack-encore-bundle` to start using
    Symfony UX #} {% block stylesheets %}
    {{
      encore_entry_link_tags("app")
    }}
    {% endblock %} {% block javascripts %}
    {{
      encore_entry_script_tags("app")
    }}

    {% endblock %}
  </head>
  <body data-preloader="3">
    {% block body %}{% endblock %}
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <script src="{{ asset('frontoffice/') }}plugins/jquery.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=IntersectionObserver"></script>
    <script src="{{ asset('frontoffice/') }}plugins/plugins.js"></script>
    <script src="{{ asset('frontoffice/') }}js/functions.js"></script>
  </body>
</html>
